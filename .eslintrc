{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true,
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "no-console": "off",
    "comma-dangle": 0,
    "object-curly-spacing": "off",
    "object-curly-newline": "off",
    "prefer-template":"off",
    "semi":"off",
    "arrow-body-style":"off",
    "global-require": "off",
    "eol-last":"off",
    "comma-spacing": "off",
    "camelcase":"off",
    "block-spacing":"off"
  }
}
